{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ],
    "removeComments": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "components/*": [ "src/components/*" ],
      "containers/*": [ "src/containers/*" ],
      "context/*": [ "src/context/*" ],
      "pages/*": [ "src/pages/*" ],
    }
  }
}
